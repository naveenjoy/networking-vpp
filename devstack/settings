enable_service vpp-agent

MECH_VPP_DIR="$DEST/networking-vpp"
MECH_VPP_BIN_DIR=$(get_python_exec_prefix)
VPP_CP_BINARY="$MECH_VPP_BIN_DIR/vpp-agent"

#A comma separated list of agent URL:ports
#Specify IP addresses only
MECH_VPP_AGENTLIST=${MECH_VPP_AGENTLIST:-'http://localhost:2704/'}

#A comma separated list of physical networks to interfaces available for use as VLAN trunks
MECH_VPP_PHYSNETLIST=${MECH_VPP_PHYSNETLIST:-physnet:GigabitEthernet2/2/0}

# If network type is flat, this is a list of flat network interfaces available for use
# to create flat networks on the node. Vpp driver will use one of the available
# interfaces for physical networks created using any arbitrary physical-network name
FLAT_NETWORK_IF=${FLAT_NETWORK_IF:-'GigabitEthernet2/2/1,GigabitEthernet2/2/2'}

VXLAN_SRC_ADDR=${VXLAN_SRC_ADDR:-}
VXLAN_BCAST_ADDR=${VXLAN_BCAST_ADDR:-}
VXLAN_VRF=${VXLAN_VRF:-1}
